-;;     (compat "29.1.4.4")
-;;     (seq "2.24")
-;;     (transient "0.5.0")
-;;     (with-editor "3.3.2"))
+;; Package-Requires: ((emacs "25.1") (compat "29.1.4.4") (seq "2.24") (transient "0.5.0") (with-editor "3.3.2"))
 
