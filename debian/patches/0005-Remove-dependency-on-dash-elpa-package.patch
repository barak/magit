From: =?utf-8?q?R=C3=A9mi_Vanicat?= <vanicat@debian.org>
Date: Sun, 23 Aug 2015 16:53:07 +0200
Subject: Remove dependency on dash elpa package

Currently, in Debian dash-el is not plugged into the elpa package
system.
Forwarded: not-needed
---
 Makefile            | 7 ++-----
 lisp/git-commit.el  | 2 +-
 lisp/magit-popup.el | 2 +-
 lisp/magit.el       | 2 +-
 lisp/with-editor.el | 2 +-
 5 files changed, 6 insertions(+), 9 deletions(-)

diff --git a/Makefile b/Makefile
index 4e7db42..47f159c 100644
--- a/Makefile
+++ b/Makefile
@@ -165,8 +165,7 @@ elpa: $(ELPA_ARCHIVES)
 define with_editor_pkg
 (define-package "with-editor" "$(VERSION)"
   "Use the Emacsclient as $$EDITOR"
-  '((emacs "$(EMACS_VERSION)")
-    (dash "$(DASH_VERSION)")))
+  '((emacs "$(EMACS_VERSION)")))
 endef
 # '
 export with_editor_pkg
@@ -189,8 +188,7 @@ git-commit-$(VERSION).el:
 define magit_popup_pkg
 (define-package "magit-popup" "$(VERSION)"
   "Define prefix-infix-suffix command combos"
-  '((emacs "$(EMACS_VERSION)")
-    (dash "$(DASH_VERSION)")))
+  '((emacs "$(EMACS_VERSION)")))
 endef
 # '
 export magit_popup_pkg
@@ -212,7 +210,6 @@ define magit_pkg
 (define-package "magit" "$(VERSION)"
   "A Git porcelain inside Emacs"
   '((emacs "$(EMACS_VERSION)")
-    (dash "$(DASH_VERSION)")
     (with-editor "$(VERSION)")
     (git-commit "$(VERSION)")
     (magit-popup "$(VERSION)")))
diff --git a/lisp/git-commit.el b/lisp/git-commit.el
index a0cf0a0..2303230 100644
